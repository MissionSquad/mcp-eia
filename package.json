{
  "name": "mcp-eia",
  "version": "1.0.2",
  "description": "MCP server for the U.S. Energy Information Administration (EIA) API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-eia": "./dist/index.js"
  },
  "scripts": {
    "gen:api-types": "openapi-typescript ./eia-swagger.yaml --output ./src/types/eia-api.d.ts",
    "clean": "rm -rf dist build",
    "build": "npm run clean && npm run gen:api-types && tsc && chmod +x dist/index.js",
    "start": "node dist/index.js",
    "dev": "tsc --watch & node --watch dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "inspect": "fastmcp inspect src/index.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "eia",
    "energy",
    "server",
    "llm",
    "ai"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "@missionsquad/common": "^1.0.2",
    "@missionsquad/fastmcp": "^1.1.0",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "axios": "^1.7.2",
    "dotenv": "^16.4.7",
    "qs": "^6.12.1",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/qs": "^6.9.15",
    "@types/uuid": "^10.0.0",
    "openapi-typescript": "^6.7.6",
    "typescript": "^5.3.3"
  }
}
